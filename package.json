{
  "name": "@herbsjs/herbs-cli",
  "version": "1.10.3",
  "description": "herbs CLI",
  "bin": {
    "herbs": "bin/herbs"
  },
  "scripts": {
    "format": "prettier --write **/*.{js,json} && standard --fix",
    "lint": "npx eslint \"**/*.{js,jsx}\" --quiet",
    "lint:fix": "npx eslint \"**/*.{js,jsx}\" --fix",
    "test": "mocha __tests__ --timeout 90000",
    "semantic-release": "semantic-release",
    "commit": "npm run lint && cz"
  },
  "engines": {
    "node": ">= 14.x"
  },
  "files": [
    "src",
    "LICENSE",
    "readme.md",
    "docs",
    "bin"
  ],
  "license": "MIT",
  "dependencies": {
    "@herbsjs/herbs": "^1.4.2",
    "@herbsjs/herbs2gql": "^2.0.1",
    "@herbsjs/herbs2knex": "^1.4.1",
    "@herbsjs/herbs2mongo": "^1.0.2",
    "@herbsjs/herbs2rest": "^2.0.1",
    "@herbsjs/herbsshelf": "^2.1.2",
    "apollo-server": "^3.6.1",
    "apollo-server-express": "^3.6.1",
    "cors": "^2.8.5",
    "deepmerge": "^4.2.2",
    "dotenv": "^12.0.3",
    "express": "^4.17.2",
    "git-user-name": "^2.0.0",
    "glob": "^7.2.0",
    "gluegun": "^4.7.1",
    "graphql": "^15.8.0",
    "graphql-tools": "^8.2.0",
    "inquirer": "^8.2.0",
    "lodash.camelcase": "^4.3.0",
    "lodash.startcase": "^4.4.0",
    "mongodb": "^4.3.0",
    "nodemon": "^2.0.15",
    "pg": "^8.7.1",
    "tedious": "^14.0.0"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^8.0.2",
    "@semantic-release/npm": "^8.0.3",
    "@semantic-release/release-notes-generator": "^10.0.3",
    "@babel/eslint-parser": "^7.16.5",
    "chai": "^4.3.4",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "mocha": "^9.1.4",
    "nyc": "^15.1.0",
    "prettier": "^2.5.1",
    "semantic-release": "^18.0.1"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "standard": {
    "ignore": [
      "__tests__/*"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/herbsjs/herbs-cli"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
